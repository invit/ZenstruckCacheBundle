{
    "name": "zenstruck/cache-bundle",
    "description": "Provides a httpcache warmup command for Symfony2",
    "keywords": ["cache", "caching", "httpcache"],
    "homepage": "http://zenstruck.com/project/ZenstruckCacheBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony" : "~2.1"
    },
    "require-dev": {
        "kriswallsmith/buzz": ">= 0.6",
        "guzzle/http": "*"
    },
    "autoload": {
        "psr-0": { "Zenstruck\\Bundle\\CacheBundle": "" }
    },
    "suggest": {
        "kriswallsmith/buzz": "To use Buzz to fetch urls",
        "guzzle/http": "To use Guzzle to fetch urls",
        "dpn/xml-sitemap-bundle": "Create a sitemap with Symfony"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "target-dir": "Zenstruck/Bundle/CacheBundle"
}
